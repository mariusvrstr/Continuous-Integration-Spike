<?xml version="1.0" encoding="utf-8" ?>
 <configuration>
  <configSections>
    <section name="unity" type="Microsoft.Practices.Unity.Configuration.UnityConfigurationSection, Unity.Configuration"/>
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  
  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <container>
      <!-- <register type="Spike.Adapters.Contracts.IUserAdapter, Spike.Contracts" mapTo="Spike.Adapters.UserAdapter, Spike.Adapters" /> -->
      
      <register type="Spike.Contracts.ISecurityOrchestration, Spike.Contracts" mapTo="Spike.Stubs.Orchestrations.SecurityOrchestrationStub, Spike.Stubs" name="StubbedVersion" />
      <register type="Spike.Contracts.ISecurityOrchestration, Spike.Contracts" mapTo="Spike.Orchestrations.SecurityOrchestration, Spike.Orchestrations" name="LatestVersion" />
    </container>
  </unity>

   <connectionStrings>
     <add name="Database.DataContext" providerName="System.Data.SqlClient" connectionString="Server=.;Database=CiData; Integrated Security=True;" />
   </connectionStrings>
   <entityFramework>
     <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
       <parameters>
         <parameter value="v11.0" />
       </parameters>
     </defaultConnectionFactory>
     <providers>
       <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
     </providers>
   </entityFramework>
   
</configuration>